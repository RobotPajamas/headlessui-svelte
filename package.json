{
  "name": "@robotpajamas/headlessui-svelte",
  "version": "2.2.0-beta.3",
  "description": "An unofficial port of the HeadlessUI component library to Svelte 5+.",
  "homepage": "https://headlessui-svelte.com",
  "license": "MIT",
  "author": {
    "name": "SJ",
    "email": "suresh@robotpajamas.com",
    "url": "https://sureshjoshi.com"
  },
  "repository": "github:robotpajamas/headlessui-svelte",
  "bugs": {
    "url": "https://github.com/robotpajamas/headlessui-svelte/issues"
  },
  "keywords": [
    "a11y",
    "accessibility",
    "components",
    "headless",
    "headless-ui",
    "headlessui",
    "svelte5",
    "svelte",
    "svelte-components",
    "sveltejs",
    "tailwindcss"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && pnpm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "pnpm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "deno fmt",
    "lint": "deno fmt --check && deno lint",
    "test": "pnpm run test:unit -- --run && pnpm run test:e2e",
    "test:e2e": "playwright test",
    "test:unit": "vitest"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "svelte": ">=5"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.46.5",
    "@sveltejs/package": "^2.5.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/vite": "^4.1.14",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.2.8",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.8.0",
    "happy-dom": "^20.0.1",
    "jsdom": "^27.0.0",
    "publint": "^0.3.14",
    "svelte": "^5.40.0",
    "svelte-check": "^4.3.3",
    "tailwindcss": "^4.1.14",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.10",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.0.0"
  }
}
